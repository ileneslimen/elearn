ector(state => state.profile.profile)\r\nuseEffect(() => {\r\n   dispatch(getProfilById(match.params.id))\r\n}, [])\r\n \r\n    return (\r\n        <Fragment>\r\n        <Fragment>\r\n        {profile===null|| loading? <Spinner/>:<Fragment> <Link to='/profiles' className ='btn btn-light'><i class=\"fas fa-arrow-left\"></i> Back To Profiles</Link> </Fragment>}    \r\n        </Fragment>\r\n        <div class=\"profile-grid my-1\">\r\n            <ProfileTop profile={profile}/>\r\n            <ProfileAbout profile={profile}/>\r\n\r\n        </div>\r\n        </Fragment>  )\r\n}\r\n","C:\\Users\\moezm\\Desktop\\elearn\\desktop\\ilen\\client\\src\\Components\\Uploadfiles\\FileUpload.js",[],"C:\\Users\\moezm\\Desktop\\elearn\\desktop\\ilen\\client\\src\\Components\\profile-form\\EditProfile.js",[],"C:\\Users\\moezm\\Desktop\\elearn\\desktop\\ilen\\client\\src\\actions\\alert.js",[],"C:\\Users\\moezm\\Desktop\\elearn\\desktop\\ilen\\client\\src\\actions\\types.js",[],"C:\\Users\\moezm\\Desktop\\elearn\\desktop\\ilen\\client\\src\\actions\\profile.js",["349"],"import axios from 'axios';\r\nimport {setAlert} from './alert'\r\nimport {\r\n    GET_PROFILE,\r\n    GET_PROFILES,\r\n    PROFILE_ERROR,\r\n    CLEAR_PROFILE,\r\n    ACCOUNT_DELETED\r\n} from './types'\r\n\r\n// get current user's profile \r\nexport const getCurrentProfile=()=>async dispatch=>{\r\n    try {\r\n        const res=await axios.get('/api/profile/me');\r\n        dispatch({\r\n            type:GET_PROFILE,\r\n            payload:res.data\r\n        })\r\n    } catch (err) {\r\n        dispatch({\r\n            type:PROFILE_ERROR,\r\n            payload:{msg:'fail', status:err.response.status}\r\n        })\r\n    }\r\n}\r\n// Create or Update Profile \r\nexport const  CreateProfil =(formData, history, edit=false) => async dispatch =>{\r\n    try {\r\n        const config ={\r\n            headers:{\r\n                'Content-Type':'application/json'\r\n            }\r\n        }\r\n        const res= awa