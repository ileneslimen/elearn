patch({\r\n            type:PROFILE_ERROR,\r\n            payload:{msg:'fail', status:err.response.status}\r\n        })\r\n    }\r\n\r\n        \r\n    }}\r\n    // get all profiles\r\nexport const getProfiles=()=>async dispatch=>{\r\n    dispatch({type:CLEAR_PROFILE})\r\n    try {\r\n        const res=await axios.get('/api/profile');\r\n        dispatch({\r\n            type:GET_PROFILES,\r\n            payload:res.data,\r\n           \r\n        })\r\n        \r\n    } catch (err) {\r\n        const errors=err.response.data.errors;\r\n        if (errors){\r\n            errors.forEach(error => dispatch(setAlert(error.msg, 'danger')));}\r\n        dispatch({\r\n            type:PROFILE_ERROR,\r\n            payload:{msg:'fail', status:err.response.status}\r\n        })\r\n    }\r\n}\r\n//get profil by id\r\nexport const getProfilById=userId=>async dispatch=>{\r\n    try {\r\n     const res= await axios.get(`/api/profile/user/${userId}`)   \r\n     dispatch({\r\n         type:GET_PROFILES,\r\n         payload:res.data\r\n     })\r\n    } catch (err) {\r\n        dispatch({\r\n            type:PROFILE_ERROR,\r\n            payload:{msg:'fail', status:err.response.status}\r\n        })  \r\n    }\r\n}","C:\\Users\\moezm\\Desktop\\elearn\\Desktop\\ilen\\client\\src\\reducers\\index.js",[],"C:\\Users\\moezm\\Desktop\\elearn\\Desktop\\ilen\\client\\src\\reducers\\alert.js",[],"C:\\Users\\moezm\\Desktop\\elearn\\Desktop\\ilen\\client\\src\\reducers\\profile.js",[],"C:\\Users\\moezm\\Desktop\\elearn\\Desktop\\ilen\\client\\src\\reducers\\auth.js",[],"C:\\Users\\moezm\\Desktop\\elearn\\Desktop\\ilen\\client\\src\\Components\\profile-form\\EditProfile.js",["119","120"],"C:\\Users\\moezm\\Desktop\\elearn\\Desktop\\ilen\\client\\src\\Components\\profile-form\\CreateProfile.js",["121"],"import React, {useState} from 'react'\r\n// import PropTypes from 'prop-types'\r\nimport {withRouter,Link}  from 'react-router-dom'\r\nimport { Fragment } from 'react';\r\nimport { useDispatch}